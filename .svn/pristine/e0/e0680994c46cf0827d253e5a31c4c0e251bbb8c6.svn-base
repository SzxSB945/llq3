{
  "name": "client",
  "version": "1.0.0",
  "description": "include server and client ,based on koa2 and parcel react redux ",
  "main": "index.js",
  "scripts": {
    "start": "cd client && cross-env NODE_ENV=development parcel map.js -p 3001 --out-dir ../release/dist",
    "release": "cd client && cross-env NODE_ENV=development parcel build map.js --out-dir ../release/dist",
    "server": "cd src && node server.js"
  },
  "author": "Tangpriest",
  "license": "MIT",
  "devDependencies": {
    "aphrodite": "^1.2.5",
    "autoprefixer": "^8.1.0",
    "axios": "^0.18.0",
    "babel-preset-react-app": "^3.1.1",
    "echarts": "^4.2.1",
    "jquery": "^3.4.1",
    "mockjs": "^1.0.1-beta3",
    "parcel-bundler": "^1.6.2",
    "react": "^16.2.0",
    "react-animations": "^1.0.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "dependencies": {
    "koa": "^2.5.2",
    "koa-static": "^3.0.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  }
}
